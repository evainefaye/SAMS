{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-const-assign": "warn",
        "no-compare-neg-zero": "warn",
        "no-constant-condition": "warn",
        "no-undef": "warn",
        "no-unused-vars": "warn",
        "comma-spacing": "warn",
        "no-trailing-spaces": "warn",
        "no-this-before-super": "warn",
        "no-unreachable": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "no-extra-semi": "warn",
        "no-extra-parens": "warn",
        "semi": [
            "error",
            "always"
        ],
        "no-irregular-whitespace": "warn",
        "quotes": ["warn", "single", { "avoidEscape": true }],
        "indent": ["warn", 4]

    },
    "globals": {
        "io":  true,
        "socket": true,
        "SASHA": true,
        "wf": true,
        "html2canvas": true,
        "moment": true,
        "define": true,
        "Cookies": true,
        "importScripts": true
    }
}